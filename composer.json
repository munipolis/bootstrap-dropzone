{
    "name": "vojtys/bootstrap-dropzone",
    "description": "Bootstrap Dropzonejs (www.dropzonejs.com) for Nette",
    "type": "library",
    "license": ["BSD-3-Clause"],
    "homepage": "https://github.com/vojtys/bootstrap-dropzone",
    "authors": [
        {
            "name": "Vojtěch Sedláček"
        }
    ],
    "suggest": {
        "ext-openssl": "To make requests via https"
    },
    "require": {
        "php": ">= 5.4",
        "nette/di": ">=2.3.0",
        "nette/application": ">=2.3.0",
        "nette/http": ">=2.3.0"
    },
    "require-dev": {
        "nette/tester": "~1.4.0"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "config": {
        "vendor-dir": "vendor"
    }
}